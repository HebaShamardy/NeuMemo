VITE_FIREBASE_MEASUREMENT_ID=

# Node scripts / server-side (do NOT prefix with VITE_)
FIREBASE_API_KEY=
FIREBASE_AUTH_DOMAIN=
FIREBASE_PROJECT_ID=
